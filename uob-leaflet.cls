% Colour commands
\newcommand{\sectioncolor}{DarkYellow}
\newcommand{\bannercolor}{BrightYellow}
\newcommand{\leafletcolor}{\sectioncolor}
\newcommand{\titlecolor}{\bannercolor}

% Headings
\newcommand{\bigheading}[1]{\textcolor{\sectioncolor}{\Huge#1}\medskip\par}
\newcommand{\medheading}[1]{\textcolor{\sectioncolor}{\Large#1}\par}
\newcommand{\smallheading}[1]{\textcolor{\sectioncolor}{\bfseries#1}\par}

% Columns
\setlength{\fboxsep}{0.75em}
\newenvironment{halfcolumn}{%
  \begin{adjustbox}{valign=t}%
    \begin{minipage}{\dimexpr0.5\linewidth - \fboxsep}}{%
    \end{minipage}%
  \end{adjustbox}%
}
\newenvironment{thirdcolumn}{%
  \begin{adjustbox}{valign=t}%
    \begin{minipage}{\dimexpr0.333\linewidth - \fboxsep}}{%
    \end{minipage}%
  \end{adjustbox}%
}

% Colour block
\newcommand{\colorblock}[2][Stone!30]{\colorbox{#1}{\begin{minipage}{\dimexpr\linewidth - 2\fboxsep}#2\end{minipage}}}

% Paragraphs are separated by vertical space rather than indents
\newlength{\df@parindent}
\setlength{\df@parindent}{0pt}
\setlength{\parindent}{\df@parindent}
\newlength{\df@parskip}
\setlength{\df@parskip}{1.5ex plus 0.5ex minus 0.5ex}
\setlength{\parskip}{\df@parskip}
\newcommand{\@minipagerestore}{\setlength{\parskip}{\df@parskip}\setlength{\parindent}{\df@parindent}}

% Headers
\def\ps@firstpage{
  \def\@oddhead{
    \begin{tikzpicture}[remember picture, overlay]
      \node[anchor=north west, inner sep=0pt, xshift=15mm, yshift=-15mm]
        at (current page.north west) {
          \parbox{\linewidth}{
            \tikz\pic{uoblogo};
            \vspace{8mm}
            \par
            \textbf{\color{\leafletcolor}\fontsize{48pt}{60pt}\selectfont\@leaflet}
            \vspace{2mm}
            \par
            {\color{\titlecolor}\Large\@title}
            \par
            \begin{pgfpicture}
              \pgfsetlinewidth{2pt}
              \pgfpathmoveto{\pgfpointorigin}
              \pgfpathlineto{\pgfpoint{\dimexpr\linewidth - 2pt}{0mm}}
              \pgfsetroundcap
              \pgfsetdash{{0pt}{4pt}}{0pt}
              \pgfusepath{stroke}
            \end{pgfpicture}
          }
        };
      \node[anchor=north east, inner sep=0pt, xshift=-15mm, yshift=-15mm]
        at (current page.north east) {\textbf{\Large\@date}};
    \end{tikzpicture}
    \hfill
  }
  \def\@oddfoot{
    \begin{tikzpicture}[remember picture, overlay]
      \node[anchor=south west, inner sep=0pt, xshift=5mm, yshift=5mm]
        at (current page.south west) {
          \begin{pgfpicture}
            \pgfpathrectangle{\pgfpointorigin}{\pgfpoint{\dimexpr\paperwidth - 10mm}{26mm}}
            \pgfsetfillcolor{\bannercolor}
            \pgfusepath{fill}
          \end{pgfpicture}
        };
      \node[anchor=south west, inner sep=0pt, xshift=15mm, yshift=12mm]
        at (current page.south west) {\parbox{\linewidth}{%
            \color{white}
            \@address
            \par
            \@contact
            \smallskip
            \par
            \textbf{\@web}
          }};
    \end{tikzpicture}
    \hfill
  }
}
\def\ps@followingpages{
  \def\@oddhead{
    \begin{tikzpicture}[remember picture, overlay]
      \node[anchor=north west, inner sep=0pt, xshift=15mm, yshift=-15mm]
        at (current page.north west) {
          \parbox{\linewidth}{
            \textbf{\LARGE\@title}
            \hfill
            \textbf{\Large\@date}
          }
        };
    \end{tikzpicture}
    \hfill
  }
  \def\@oddfoot{
    \begin{tikzpicture}[remember picture, overlay]
      \node[anchor=south west, inner sep=0pt, xshift=15mm, yshift=33mm]
        at (current page.south west) {
          \begin{pgfpicture}
            \pgfsetlinewidth{2pt}
            \pgfpathmoveto{\pgfpointorigin}
            \pgfpathlineto{\pgfpoint{\dimexpr\linewidth - 2pt}{0mm}}
            \pgfsetroundcap
            \pgfsetdash{{0pt}{4pt}}{0pt}
            \pgfusepath{stroke}
          \end{pgfpicture}};      
      \node[anchor=south east, inner sep=0pt, xshift=-15mm, yshift=12mm]
        at (current page.south east) {\tikz\pic{uoblogo};};
      \node[anchor=south west, inner sep=0pt, xshift=15mm, yshift=12mm]
        at (current page.south west) {\parbox{\linewidth}{%
            \@address
            \par
            \@contact
            \smallskip
            \par
            \textbf{\@web}
          }};
    \end{tikzpicture}
    \hfill
  }
}

\pagestyle{followingpages}

% Set up the first page
\AtBeginDocument{%
  \thispagestyle{firstpage}%
  \newgeometry{margin=15mm, head=50mm, headsep=4mm, foot=20mm, includeheadfoot}
}

% Enable PDF output
\pdfoutput=1

% Sans-serif fonts as default
\renewcommand{\familydefault}{\sfdefault}

