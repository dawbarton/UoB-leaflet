\RequirePackage{geometry}           % Set the page size
\RequirePackage{microtype}          % Micro-typography for sorting out spacing
\RequirePackage{graphicx}           % Graphics
\RequirePackage{xcolor}             % Colours
\RequirePackage{tikz}               % Graphics primatives
\RequirePackage{adjustbox}          % Adjustable boxes

