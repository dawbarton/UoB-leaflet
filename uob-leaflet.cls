\setlength{\parindent}{0em}
\setlength{\parskip}{1.5ex plus 0.5ex minus 0.5ex}

% Remove section numbers
\setcounter{secnumdepth}{0}

% Dotted lines
\newcommand{\dottedhline}{%
  \begin{pgfpicture}
    \pgfsetlinewidth{1pt}
    \pgfpathmoveto{\pgfpointorigin}
    \pgfpathlineto{\pgfpoint{\dimexpr\linewidth - 1pt}{0mm}}
    \pgfsetroundcap
    \pgfsetdash{{0pt}{2pt}}{0pt}
    \pgfusepath{stroke}
  \end{pgfpicture}\hfill%
}
\newcommand{\dottedvline}[1]{
  \begin{adjustbox}{valign=t}%
    \begin{pgfpicture}
      \pgfsetlinewidth{1pt}
      \pgfpathmoveto{\pgfpointorigin}
      \pgfpathlineto{\pgfpoint{0mm}{\dimexpr1pt - #1}}
      \pgfsetroundcap
      \pgfsetdash{{0pt}{2pt}}{0pt}
      \pgfusepath{stroke}
    \end{pgfpicture}%
  \end{adjustbox}
}

